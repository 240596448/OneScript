Перем юТест;

Функция ПолучитьСписокТестов(Знач Тесты) Экспорт
	юТест = Тесты;
	
	Список = Новый Массив;
	Список.Добавить("ТестДолжен_ИспользоватьПодключенныйМодуль");
	
	Возврат Список;
	
КонецФункции

Процедура ТестДолжен_ИспользоватьПодключенныйМодуль() Экспорт
	
	А = "";
	ОбщиеФункции.Скажи("Привет", А); 
	юТест.ПроверитьРавенство("Привет", А);
	юТест.ПроверитьРавенство(1, ОбщиеФункции.Один());
	
КонецПроцедуры
